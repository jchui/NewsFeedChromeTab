{
  "manifest_version": 2,
  "name": "New Tab Extension",
  "description": "A new tab window with an integrated newsfeed",
  
  "author": "Jeremy Chui",
  
  "default_locale": "en",
  "version": "0.1",
  "permissions": [
    "http://api.jchui.me/minerva/"
  ],
  "background": {
    "scripts": ["js/vendor/angular.min.js"]
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "chrome_url_overrides" : {
    "newtab": "tab.html"
  }
}